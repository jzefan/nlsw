extends ../layout
block content
  .page-header
    include tool_group

  div(style="overflow:scroll;min-height:65%;height:65vh")
    .row.margin
      table#data-table.table.table-bordered(style='margin-bottom: 0px !important')
        thead
          tr
            th(style='width:20px;max-width:25px;min-width:10px;') 行
            th 车船号
            th 类别
            th 性质
            th 联系人
            th 联系人电话
        tbody#data-tbody
          if dData
            each vehicle, index in dData
              tr
                td= index + 1
                td= vehicle.name
                td= vehicle.veh_type
                td= vehicle.veh_category
                td= vehicle.contact_name
                td= vehicle.phone

  include data_dialog
  script.
    var local_data = !{JSON.stringify(dData)};
    var local_operObject = !{JSON.stringify(dOperationObject)};

